// APP BACKGROUND COLOR
Ti.UI.setBackgroundColor("#000");


//MAIN WINDOW
var mainWindow = Ti.UI.createWindow ({
	backgroundColor: "#eee"
});

var imageBackground = Ti.UI.createImageView ({
		images: "images/background/navy-submarine-1920x1200.jpg",
		layout: "vertical"
	});


mainWindow.add(imageBackground);
mainWindow.open();


